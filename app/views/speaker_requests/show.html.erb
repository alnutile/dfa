<p id="notice"><%= notice %></p>

<h3><%= @speaker_request.title %></h3>

<dl class="dl-horizontal">
  <dt>Title</dt>
  <dd><%= @speaker_request.title %></dd>

  <dt>Description</dt>
  <dd><%= @speaker_request.content %></dd> 

  <dt>Date</dt>
  <dd><%= @speaker_request.date %></dd>

  <dt>Speaker</dt>
  <dd>
      <%= simple_format speakerName(@speaker_request) %>
  </dd>
  <dt>Location</dt>
  <dd> 
    <%= simple_format locationName(@speaker_request) %>
  </dd>
</dl>