<h1>Listing speaker_requests</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th>Date</th>
    <th>Speaker</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @speaker_requests.each do |speaker_request| %>
  <tr>
    <td><%= speaker_request.title %></td>
    <td><%= speaker_request.content %></td>
    <td><%= speaker_request.date %></td>
    <td><%= speaker_request.speaker_id %></td>
    <td><%= speaker_request.location_id %></td>
    <td><%= link_to 'Show', speaker_request %></td>
    <td><%= link_to 'Edit', edit_speaker_request_path(speaker_request) %></td>
    <td><%= link_to 'Destroy', speaker_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Speaker request', new_speaker_request_path %>
